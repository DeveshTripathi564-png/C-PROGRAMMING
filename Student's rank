#include<stdio.h>
 int main ()
 {
    int a,b,c,d,e ;
    printf("Enter the No. of a,b,c,d,e ");
    scanf ("%d %d %d %d %d",&a,&b,&c,&d,&e);
    int p=a+b+c+d+e;
    if ( p>=450&&p<=500){
        printf("the rank of student in class 1st");
    }else
        if (p>=350&&p<450){
            printf("The rank of student in class 2nd ");
        }else
            if (p>=250&&p<350){
                printf("The rank of student in class 3rd");
            }else
                if(p>=150&&p<250){
                   { printf ("the student passs");}
                }else
                    if (p>100&&p<150)
                   { printf("the student needs improvment");
                } else {
                    printf ("the student fall ");
                }
           return 0;
 }
